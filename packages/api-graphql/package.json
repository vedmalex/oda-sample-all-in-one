{
  "name": "api-graphql",
  "version": "1.0.0",
  "description": "sample api for uiTemplate",
  "main": "dist/app.js",
  "scripts": {
    "watch": "tsc -w -p ./",
    "start": "node bin/www",
    "postinstall": "npm run compile",
    "generate": "node dist/generate.js",
    "validate": "node dist/validate.js",
    "genUml": "node dist/genUml.js",
    "debug": "node --inspect-brk bin/www",
    "updateSchema": "node updateSchema.js",
    "compile": "./compile.sh 2> /dev/null",
    "clean": "./clean.sh",
    "collector": "cd src/graphql-gen/system/queries; graphql-document-collector '**/*.graphql' > ../../../../data/seed-queries.json; cd ../../../.. ; node dist/seeds/validate.js",
    "restoreData": "node dist/seeds/restoreDirect.js data/dump.json",
    "dumpData": "node dist/seeds/dumpDirect.js data/dump_1.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-upload-server": "^2.0.4",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tag": "^2.4.2",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "mysql2": "^1.4.0",
    "oda-aor-rest": "0.1.25",
    "oda-api-common": "^0.2.10",
    "oda-api-graphql": "^0.4.18",
    "oda-gen-common": "0.0.16",
    "oda-gen-graphql": "^0.4.81",
    "oda-isomorfic": "0.0.13",
    "oda-lodash": "^0.0.56",
    "oda-model": "^0.0.33",
    "sqlite3": "^3.1.13",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.0",
    "@types/inflected": "^1.1.29",
    "@types/mongoose": "^4.7.20",
    "@types/node": "^8.0.24",
    "@types/passport": "^0.3.3",
    "graphql-document-collector": "^0.1.3",
    "typescript": "^2.4.2"
  }
}